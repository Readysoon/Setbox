[MASTER]
disable=
    C0114, # missing-module-docstring
    C0115, # missing-class-docstring
    C0116, # missing-function-docstring
    E0401, # import-error (for some reason, pylint doesn't like the way I import modules in my Django project)
    W0718, # broad-exception-caught
    W0719, # broad-exception-raised

ignore=
    migrations,
    README.md,
    requirements.txt,
    venv,
    tests